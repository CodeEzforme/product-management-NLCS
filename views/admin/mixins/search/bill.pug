mixin search(phone)
  form(id="form-search-bill")
    .input-group
      input(
        type="text"
        placeholder="Nhập số điện thoại"
        name="keyword"
        class="form-control"
        value=phone
      )
      .input-group-append
        button(
          class="btn btn-success"
          type="submit"
        ) Tìm